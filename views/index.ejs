<%- include('./parts/head') %>
<%- include('./parts/header') %>

<div id="chat_row" class="row">
    <div id="users_window" class="d-none d-lg-block col-lg-2">
        <video id="vid" autoplay></video>
    </div>
    <div id="chat_window" class="col">

        <ul id="messages_container" class="list-unstyled">
            <!-- parts/ message.ejs -->
            <% datas.messages.forEach(function(message, index){ %>
                <%- include('parts/message', {post : message, i : index}); %>
            <% }); %>
        </ul>
        
    </div>
    <div id="connect_window" class="d-none d-lg-block col-lg-2">

    </div>
</div>

<%- include('./parts/footer') %>